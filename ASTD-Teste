function Tp(lugar)
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = lugar.CFrame
	end

function Spawn(unit)
	args = {
	[1] = "SpawnAllyODD",
	[2] = unit
	}
	game:GetService("ReplicatedStorage").Remotes.Server:InvokeServer(unpack(args))
	end

function Tp2(x, y, z)
	getgenv().coordenada = CFrame.new(x, y, z)
	getgenv().experiencia = game:GetService("Players")
	getgenv().objeto = experiencia.LocalPlayer.Character.HumanoidRootPart
	getgenv().objeto.CFrame = coordenada
	end

while wait() do
	if game.Players.LocalPlayer.Character and game:GetService("Workspace").Queue.Sacri.ExamDoor then
		wait(5)
		Tp(game:GetService("Workspace").Queue.Sacri.ExamDoor)
		break
	end
	if game.Players.LocalPlayer.Character and not game:GetService("Workspace").Queue.Sacri.ExamDoor then
		Spawn(Unidade1)
		Spawn(Unidade2)
		Spawn(Unidade3)
		break
	end
end
