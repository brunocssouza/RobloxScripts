function Tp(lugar)
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = lugar.CFrame
	end

function Spawn(unit)
	args = {
	[1] = "SpawnAllyODD",
	[2] = unit
	}
	game:GetService("ReplicatedStorage").Remotes.Server:InvokeServer(unpack(args))
	end

function Critical()
	local args = {
	    [1] = "Critical"
	}

	game:GetService("ReplicatedStorage").Remotes.Oddessey:FireServer(unpack(args))
	wait(1)
	end

while wait() do
	if game.Players.LocalPlayer.Character then
		if game.Workspace:FindFirstChild("Queue") ~= nil then
			Tp(game:GetService("Workspace").Queue.Sacri.ExamDoor)
			break
		end
		if game.Workspace:FindFirstChild("Queue") == nil then
			Spawn(Unidade1)
			Spawn(Unidade2)
			Spawn(Unidade3)
			while wait() do
				Critical()
			end
		end
	end
end
