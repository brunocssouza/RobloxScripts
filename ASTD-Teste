function Tp(lugar)
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = lugar.CFrame
	end

function Spawn(unit)
	args = {
	[1] = "SpawnAllyODD",
	[2] = unit
	}
	game:GetService("ReplicatedStorage").Remotes.Server:InvokeServer(unpack(args))
	end

wait(10)
Tp(game:GetService("Workspace").Queue.Sacri.ExamDoor)
wait(4)
Tp(113.54932403564, 39.825588226318, -0.61703759431839)

Spawn(Unidade1)
Spawn(Unidade2)
Spawn(Unidade3)
